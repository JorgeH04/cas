
<% include partials/_header %>




<!-- Header /- -->
    <!-- Page Introduction Wrapper -->
    <div class="page-style-a">
        <div class="container">
            <div class="page-intro">
                <h2>Favoritos</h2>
                <ul class="bread-crumb">
                    <li class="has-separator">
                        <i class="ion ion-md-home"></i>
                        <a href="/">Home</a>
                    </li>
                    <li class="is-marked">
                        <a  >Favoritos</a>
                    </li>
                </ul>
            </div>
        </div>
    </div>
    <!-- Page Introduction Wrapper /- -->
    <!-- Wishlist-Page -->
    <div class="page-wishlist u-s-p-t-80">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <!-- Products-List-Wrapper -->
                    <div class="table-wrapper u-s-m-b-60">
                        <table>
                            <thead>
                                <tr>
                                    <th>Producto</th>
                                    <th>Precio</th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>
                             

                                <% likedofertauno.forEach(function (ofertaun) { %>
                                <tr class="p" data-id="<%= ofertaun.id %>">
                                    <td>
                                        <div class="cart-anchor-image">
                                            <a>
                                                <img src="<%= ofertaun.image %>" alt="Product">
                                                <h6><%= ofertaun.title %></h6>
                                            </a>
                                        </div>
                                    </td>
                                    <td>
                                        <div class="cart-price">
                                            $<%= ofertaun.price %>
                                        </div>
                                    </td>
                                
                                    <td>
                                        <div class="action-wrapper">
                                            <a href="/addtocardofertauno/<%= ofertaun.id %>" 
                                                class="button button-outline-secondary">
                                                <i class="fa fa-shopping-cart" aria-hidden="true"></i> 
                                            </a>
                                            <a href="/like/<%= ofertaun.id %>"  
                                                class="button button-outline-secondary fas fa-trash"
                                                id="unlike-uno"  data-id="<%= ofertaun.id %>"
                                                >
                                            </a>
                                        </div>
                                    </td>
                                </tr>
                                <% }) %>

                                <% likedofertados.forEach(function (ofertaun) { %>
                                    <tr class="p" data-id="<%= ofertaun.id %>">
                                        <td>
                                            <div class="cart-anchor-image">
                                                <a >
                                                    <img src="<%= ofertaun.image %>" alt="Product">
                                                    <h6><%= ofertaun.title %></h6>
                                                </a>
                                            </div>
                                        </td>
                                        <td>
                                            <div class="cart-price">
                                                $<%= ofertaun.title %>
                                            </div>
                                        </td>
                                  
                                        <td>
                                            <div class="action-wrapper">
                                                <a href="/addtocardofertados/<%= ofertaun.id %>" 
                                                    class="button button-outline-secondary">
                                                    <i class="fa fa-shopping-cart" aria-hidden="true"></i> </a>
                                                <a href="/likedos/<%= ofertaun.id %>"  
                                                    class="button button-outline-secondary fas fa-trash"
                                                    id="btn-unlike"  data-id="<%= ofertaun.id %>"
                                                    >
                                                </a>
                                            </div>
                                        </td>
                                    </tr>
                                    <% }) %>


                                    <% likedofertatres.forEach(function (ofertaun) { %>
                                        <tr class="p" data-id="<%= ofertaun.id %>">
                                            <td>
                                                <div class="cart-anchor-image">
                                                    <a href="single-product.html">
                                                        <img src="<%= ofertaun.image %>" alt="Product">
                                                        <h6><%= ofertaun.title %></h6>
                                                    </a>
                                                </div>
                                            </td>
                                            <td>
                                                <div class="cart-price">
                                                    $<%= ofertaun.title %>
                                                </div>
                                            </td>
                                 
                                            <td>
                                                <div class="action-wrapper">
                                                    <a href="/addtocardofertatres/<%= ofertaun.id %>"
                                                         class="button button-outline-secondary">
                                                         <i class="fa fa-shopping-cart" aria-hidden="true"></i> </a>
                                                    <a href="/liketres/<%= ofertaun.id %>"  
                                                        class="button button-outline-secondary fas fa-trash"
                                                        id="btn-unlike"  data-id="<%= ofertaun.id %>"
                                                        >
                                                    </a>
                                                </div>
                                            </td>
                                        </tr>
                                        <% }) %>


                                        <% likedproduno.forEach(function (ofertaun) { %>
                                            <tr class="p" data-id="<%= ofertaun.id %>">
                                                <td>
                                                    <div class="cart-anchor-image">
                                                        <a href="single-product.html">
                                                            <img src="<%= ofertaun.image %>" alt="Product">
                                                            <h6><%= ofertaun.title %></h6>
                                                        </a>
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="cart-price">
                                                        $<%= ofertaun.title %>
                                                    </div>
                                                </td>
                                             
                                                <td>
                                                    <div class="action-wrapper">
                                                        <a href="/addtocardproduno/<%= ofertaun.id %>" 
                                                            class="button button-outline-secondary">
                                                            <i class="fa fa-shopping-cart" aria-hidden="true"></i> </a>
                                                        <a href="/likeproduno/<%= ofertaun.id %>"  
                                                            class="button button-outline-secondary fas fa-trash"
                                                            id="btn-unlike"  data-id="<%= ofertaun.id %>"
                                                            >
                                                        </a>
                                                    </div>
                                                </td>
                                            </tr>
                                            <% }) %>




 
                            </tbody>
                        </table>
                    </div>
                    <!-- Products-List-Wrapper /- -->
                </div>
            </div>
        </div>
    </div>
    <!-- Wishlist-Page /- -->


    <% include partials/_footer %>
